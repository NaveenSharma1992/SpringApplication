package beans;

public class Test {
	
	private String name;
	private int age;
	private String email;
	
	private Test() {
		System.out.println("Constructor calling.....");
	}
	public Test(String name, int age, String email) {
		this.name= name;
		this.age = age;
		this.email = email;
		
	}
	
	public void helloWorld() {
		System.out.println("Hello Spring.....");
	}
}
